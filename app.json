{
  "expo": {
    "name": "mobile",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.huunhan.mobile"
    },
    "web": {},
    "assetBundlePatterns": [
      "assets/fonts/*"
    ],
    "extra": {
      "eas": {
        "projectId": "3523781e-c227-4450-bdfd-5704f83cae18"
      }
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
